################################################################################
#(Windows only)
# Setup: type
#
#
# The setup type to be used when installing the product. Possible values are:
# 
#    Typical   - Typical: The wizard installs the agent using  
#                the default configuration. See the installation manual
#				 for details of the defaults.
#
#    Custom    - Custom: The wizard enables you to customize the configuration
#                parameters before installing the agent.
# 
# For example, to specify that the "Typical" setup type is selected, use the 
# following:
# 	 
#    SetupType=Typical
#    
# 

################################################################################
#
# Agent configuration: Port
#
# Type the port number used by the agent.
# This value is used if SecurityLevel=0
#
Port=<%= @Port %>

################################################################################
#
# Agent configuration: Port
#
# Type the port number used by the agent.
# This value is used if SecurityLevel=1
#
SecureAuth=<%= @SecureAuthPort %>

################################################################################
#
# Agent configuration: Port
#
# Type the port number used by the agent.
# This value is used if SecurityLevel=2
#
SecureAll=<%= @ClientAuthSecurePort %>

################################################################################
#
# Agent configuration: Agent Temporary Path
#
# Specify the Agent Temporary Directory (a working directory used for example for file
# download). The default location is /tmp/itlm 
# Example (Windows)
     AgentTempPath=<%= @TempPath %>
# Example (UNIX)
# AgentTempPath=/opt/IBM
# AgentInstallPath=<%= @AgentInstallPath %> 


################################################################################
#
# Agent configuration: Message Handler Address
#
# Specify the hostname or IP address of the message handler server.
#
MessageHandlerAddress=<%= @MessageHandlerAddress %>

################################################################################
#
# Agent configuration: Scan Group Name
#
# Specify the name of the scan group.
#
# This is a recommendation and is only used for the initial registration with
# the server. The server may ignore the recommendation and reassign the agent
# into a default scan group that has been designated by the inventory
# administrator on the server. The inventory administrator may also reassign
# the agent to another scan group at anytime. This value may not reflect the
# current scan group that the agent is actually assigned to if it has
# been reassigned on the server.
#
ScanGroup=<%= @ScanGroup %>

################################################################################
#
# Agent configuration: Security level
#
# Select the security level for the agent. 
# Possible values are:
# 
#   0  - Indicates that the agent communication will not be secure (HTTP)
#   1  - Indicates that the agent communication will be secure with server authentication only 
#          (using SSL with only Server Authentication feature)
#   2  - Indicates that the agent communication will be secure with both Agent and Server Authentication 
#          (using SSL with both Client and Server Authentication feature)
#
SecurityLevel=<%= @SecurityLevel %>

################################################################################
#
# Agent configuration: Use Proxy
#
# If the agent is not protected by a proxy server, set this value to "n",
# otherwise set it to "y".
#
# Possible values are:
# 
#     y  - Indicates that the agent is protected by a proxy server
#     n  - Indicates that the agent is not protected by a proxy server
# 
UseProxy=<%= @UseProxy %>

################################################################################
#
# Agent configuration: Proxy Port
#
# Specify the port number used by the proxy server.
# This Value is ignored if UseProxy = n.
#
ProxyPort=<%= @ProxyPort %>

###############################################################################
#
# Agent configuration: Proxy Address
#
# Specify the address of the proxy server.
# This Value is ignored if UseProxy = n.
#
ProxyAddress=<%= @ProxyAddress %>

################################################################################
#
# Agent configuration: FIPS Enabled.
#
# Set this value to "y" if the agent is to use FIPS. 
# 
# Permitted values are:
# 
#     y  - Indicates that the agent is using FIPS 
#     n  - Indicates that the agent is not using FIPS
# 
FipsEnabled=<%= @FipsEnabled %>

################################################################################
#
# Agent configuration: Common Inventory Technology Destination Path
#
# Specify the Common Inventory Technology installation folder. Leave this field blank in order
# to use the default value.
#
CITInstallPath=<%= @CITInstallPath %>

################################################################################
#
# Install server certificate:
#
# If you have selected secure communications (SecurityLevel>0), you can choose to install server certificate. 
#
# Possible values are:
# 
#     y, n.
#
# If you select the value "y", you must also specify if you use your own server 
# certificate or server test certificate. 
# (ServerCustomSSLCertificate).
#
# For example, to indicate that you want to install the certificate
# use the following: 
#   
#    InstallServerCertificate=y
#
InstallServerCertificate=<%= @InstallServerCertificate %>

################################################################################
#
# SSL certificate: server certificate
#
# If you have selected to supply SSL certificate (InstallServerCertificate=y), you can choose to provide your own 
# server certificate to be used by the agent for secure communications with the server.
#
# Possible values are:
# 
#     y  - Indicates that you want to provide your own server certificate.
#     n  - Indicates that you want to use the server test certificate. 
#
# You should note that the test certificate may only be used for
# test purposes as it is in the name of IBM and is insecure (the same 
# certificate is distributed to all customers).
#
# If you select the value "y", you must also supply the certificate path name
# (ServerCertFilePath).
#
# For example, to indicate that you want to use your own server certificate for SSL
# communication, use the following: 
#   
#    ServerCustomSSLCertificate=y
#
ServerCustomSSLCertificate=<%= @ServerCustomSSLCertificate %>

################################################################################
#
# SSL certificate: server certificate path name
#
# If you have selected to supply your own server certificate (ServerCustomSSLCertificate=y), 
# you must provide the path name of your own server certificate (including file name).
# e.g. ServerCertFilePath=/tmp/certificates/cert.arm
#
# The name of the certificate must be "cert.arm".
# If the path contains spaces, enclose the whole path in double-quotes. 
#
ServerCertFilePath=<%= @ServerCertFilePath %>

################################################################################
#
# Agent configuration: Agent certificate file
#
# If you have selected security level maximum, 
# you should provide agent personal certificate.
#
AgentCertFilePath=<%= @AgentCertFilePath %>

################################################################################
# Linux390 specific parameters
################################################################################
################################################################################
#
# Specify the total number of all active processors (CP and IFL) 
# in a physical machine.   
#
SystemActiveProcessors=

################################################################################
#
# If the Linux on System z image is configured to share processors, specify 
# the total number of shared processors (CP or IFL, as appropriate, not both)
# in the CEC. Enter 0 (zero) if no shared processors are used by this image.
#
SharedPoolCapacity=

################################################################################
#
# Specify the processor type that the Linux on System z image is running on.
# Possible values are:
# 
#     CP   - Indicates that your Linux on System z image is running on CP processors
#     IFL  - Indicates that your Linux on System z image is running on IFL processors
#
ProcessorType=IFL

################################################################################
#
# Specify the machine type the Linux on System z image is running on.
# Possible values are:
# 
#     z9   - Indicates that your Linux on System z image is running on System z9 or z990 or S/390
#     z10  - Indicates that your Linux on System z image is running on System z10
#
MachineType=z9
